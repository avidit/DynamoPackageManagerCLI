{
  "name": "@avidit/dpm",
  "version": "0.1.0",
  "description": "Dynamo Package Manager cli",
  "main": "./bin/dpm",
  "bin": {
    "dpm": "bin/dpm"
  },
  "directories": {
    "lib": "lib"
  },
  "os": ["win32"],
  "scripts": {
    "version:patch": "npm version patch -m \"Upgrade to v%s\"",
    "version:minor": "npm version minor -m \"Upgrade to v%s\"",
    "version:major": "npm version major -m \"Upgrade to v%s\"",
    "test": "echo \"No test specified\"",
    "prettier": "prettier --write bin/dpm \"**/*.js\"",
    "standard": "standard --fix bin/dpm \"**/*.js\""
  },
  "keywords": ["dynamo", "package", "manager", "cli", "dpm"],
  "author": "avidit",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "columnify": "^1.5.4",
    "commander": "^8.3.0",
    "got": "^11.8.5",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "prettier": "^2.3.2",
    "standard": "^16.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avidit/DynamoPackageManagerCLI.git"
  },
  "bugs": {
    "url": "https://github.com/avidit/DynamoPackageManagerCLI/issues"
  },
  "homepage": "https://github.com/avidit/DynamoPackageManagerCLI#readme"
}
