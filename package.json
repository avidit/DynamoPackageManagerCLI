{
  "name": "dpm",
  "version": "0.0.8",
  "description": "Dynamo Package Manager cli",
  "main": "./bin/dpm",
  "bin": {
    "dpm": "./bin/dpm"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "version:patch": "npm version patch -m \"Upgrade to v%s\" --no-git-tag-version",
    "version:minor": "npm version minor -m \"Upgrade to v%s\" --no-git-tag-version",
    "version:major": "npm version major -m \"Upgrade to v%s\" --no-git-tag-version",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --write bin/dpm \"**/*.js\"",
    "standard": "standard --fix bin/dpm \"**/*.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "columnify": "^1.5.4",
    "commander": "^8.3.0",
    "got": "^11.8.5",
    "xml2js": "^0.5.0"
  },
  "devDependencies": {
    "prettier": "^2.3.2",
    "standard": "^16.0.3"
  }
}
